<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationController</servlet-name>
  	<display-name>RegistrationController</display-name>
  	<description></description>
  	<servlet-class>com.buddhika.consultant.service.controller.RegistrationController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.buddhika.consultant.service.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AppointmentController</servlet-name>
  	<display-name>AppointmentController</display-name>
  	<description></description>
  	<servlet-class>com.buddhika.consultant.service.controller.AppointmentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConsultantController</servlet-name>
  	<display-name>ConsultantController</display-name>
  	<description></description>
  	<servlet-class>com.buddhika.consultant.service.controller.ConsultantController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminControllerAppointments</servlet-name>
  	<display-name>AdminControllerAppointments</display-name>
  	<description></description>
  	<servlet-class>com.buddhika.consultant.service.controller.AdminControllerAppointments</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>AdminController</servlet-name>
  	<display-name>AdminController</display-name>
  	<description></description>
  	<servlet-class>com.buddhika.consultant.service.controller.AdminController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegistrationController</servlet-name>
  	<url-pattern>/userdao</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/appointment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/getappointment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConsultantController</servlet-name>
  	<url-pattern>/getconsult</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminControllerAppointments</servlet-name>
  	<url-pattern>/getadconappointments</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminController</servlet-name>
  	<url-pattern>/postadmin</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>AdminController</servlet-name>
  	<url-pattern>/getadmin</url-pattern>
  </servlet-mapping>
  
</web-app>

